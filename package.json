{
  "name": "nextn",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --port 9002 --hostname 0.0.0.0",
    "dev:next": "next dev --port 9002 --hostname 0.0.0.0",
    "dev:web": "npm run dev:next",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "ngrok": "ngrok start --config \"%USERPROFILE%\\\\.ngrok2\\\\ngrok.yml\" cosmicdev",
    "dev:with-tunnel": "concurrently \"npm:dev\" \"npm:ngrok\"",
    "refresh": "powershell -ExecutionPolicy Bypass -File scripts/refresh-dev.ps1",
    "refresh:vip": "tsx scripts/vip-live-refresh.ts"
  },
